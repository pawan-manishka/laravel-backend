    "paths": {
        "/birthdays": {
            "get": {
                "tags": [
                    "Employments"
                ],
                "summary": "Get birthdays of employmnets",
                "description": "Returns upcoming birthdays of employments",
                "parameters": [
                    {
                        "name": "company",
                        "in": "query",
                        "description": "Company ID",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "example": "2719cde8-7019-4ca8-81f5-1b6057037307"
                        }
                    },
                    {
                        "name": "within",
                        "in": "query",
                        "description": "Comma seperated date range",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "example": "2018-01-01,2018-12-31"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successull Operation",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "400": {
                        "description": "Bad request"
                    },
                    "404": {
                        "description": "Resource Not Found"
                    },
                    "500": {
                        "description": "Internal Server Error"
                    }
                },
                "security": [
                    {
                        "passport": []
                    }
                ]
            }
        },
        "/video": {
            "post": {
                "tags": [
                    "Projects"
                ],
                "summary": "Get list of projects",
                "description": "Returns list of projects",
                "operationId": "getProjectsList",
                "responses": {
                    "200": {
                        "description": "Successful operation"
                    },
                    "401": {
                        "description": "Unauthenticated"
                    },
                    "403": {
                        "description": "Forbidden"
                    }
                }
            }
        }
    }
}